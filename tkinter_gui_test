from tkinter import *

fenetre = Tk()

canvas = Canvas(fenetre, width=500, height=300, background='white')

canvas.create_rectangle(50, 50, 100, 250, outline="black", fill='white') #do
canvas.create_rectangle(100, 50, 150, 250, outline="black", fill='white') #ré
canvas.create_rectangle(150, 50, 200, 250, outline="black", fill='white') #mi
canvas.create_rectangle(200, 50, 250, 250, outline="black", fill='white') #fa
canvas.create_rectangle(250, 50, 300, 250, outline="black", fill='white') #sol
canvas.create_rectangle(300, 50, 350, 250, outline="black", fill='white') #la
canvas.create_rectangle(350, 50, 400, 250, outline="black", fill='white') #si
canvas.create_rectangle(400, 50, 450, 250, outline="black", fill='white') #do

canvas.create_rectangle(80, 50, 120, 150, outline="black", fill='black') #do dièse
canvas.create_rectangle(130, 50, 170, 150, outline="black", fill='black') #ré dièse
canvas.create_rectangle(230, 50, 270, 150, outline="black", fill='black') #fa dièse
canvas.create_rectangle(280, 50, 320, 150, outline="black", fill='black') #sol dièse
canvas.create_rectangle(330, 50, 370, 150, outline="black", fill='black') #la dièse

canvas.pack()

fenetre.mainloop()
